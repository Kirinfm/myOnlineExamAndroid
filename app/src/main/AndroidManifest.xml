<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.onlineexam">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme">
        <activity android:name=".Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/Theme" />
        <activity android:name=".Activity.RegisterActivity" />
        <activity
            android:name=".Activity.ExamListActivity"
            android:label="@string/title_activity_main"
            android:parentActivityName=".Activity.MainActivity"
            android:theme="@style/Theme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.onlineexam.Activity.MainActivity" />
        </activity>
        <activity
            android:name=".Activity.ExamActivity"
            android:label="@string/title_activity_main"
            android:parentActivityName=".Activity.ExamListActivity"
            android:theme="@style/Theme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.onlineexam.Activity.ExamListActivity" />
        </activity>
        <activity
            android:name=".Activity.ScoreActivity"
            android:label="@string/title_activity_main"
            android:parentActivityName=".Activity.MainActivity"
            android:theme="@style/Theme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.onlineexam.Activity.MainActivity" />
        </activity>
        <activity
            android:name=".Activity.PickActivity"
            android:label="@string/title_activity_main"
            android:parentActivityName=".Activity.MainActivity"
            android:theme="@style/Theme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.onlineexam.Activity.MainActivity" />
        </activity>
        <activity
            android:name=".Activity.UpdateinfoActivity"
            android:label="@string/title_activity_main"
            android:parentActivityName=".Activity.MainActivity"
            android:theme="@style/Theme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.onlineexam.Activity.MainActivity" />
        </activity>
        <activity
            android:name=".Activity.T_MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/Theme" />
        <activity
            android:name=".Activity.EditExamActivity"
            android:label="@string/title_activity_main"
            android:parentActivityName=".Activity.T_MainActivity"
            android:theme="@style/Theme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.onlineexam.Activity.T_MainActivity" />
        </activity>
        <activity
            android:name=".Activity.EditQuestionActivity"
            android:label="@string/title_activity_main"
            android:parentActivityName=".Activity.EditExamActivity"
            android:theme="@style/Theme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.onlineexam.Activity.EditExamActivity" />
        </activity>
        <activity
            android:name=".Activity.EditStudentActivity"
            android:label="@string/title_activity_main"
            android:parentActivityName=".Activity.T_MainActivity"
            android:theme="@style/Theme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.onlineexam.Activity.T_MainActivity" />
        </activity>
        <activity
            android:name=".Activity.UpdateTeacherinfoActivity"
            android:label="@string/title_activity_main"
            android:parentActivityName=".Activity.T_MainActivity"
            android:theme="@style/Theme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.onlineexam.Activity.T_MainActivity" />
        </activity>
    </application>

</manifest>